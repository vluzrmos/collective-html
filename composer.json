{
  "name": "vluzrmos/collective-html",
  "description": "LaravelCollective Html and Form builder for Lumen.",
  "keywords": ["Lumen", "Html", "Form", "Builder"],
  "license": "MIT",
  "authors": [
    {
      "name": "Adam Engebretson",
      "email": "adam@laravelcollective.com"
    },
    {
      "name": "Taylor Otwell",
      "email": "taylorotwell@gmail.com"
    },
    {
      "name": "Vagner do Carmo",
      "email": "vluzrmos@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "laravel/lumen-framework": "~5.4",
    "illuminate/session": "~5.4",
    "illuminate/http": "~5.4",
    "illuminate/support": "~5.4"
  },
  "require-dev": {
    "mockery/mockery": "~0.9",
    "phpunit/phpunit": "~4.0",
    "vluzrmos/lumen-url-host":"~0.0"
  },
  "replace": {
    "laravelcollective/html": "self.version"
  },
  "autoload": {
    "psr-4": {
      "Collective\\Html\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  }
}
